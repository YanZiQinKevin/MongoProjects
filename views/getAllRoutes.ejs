<!DOCTYPE html>
<html>
<head>
    <title>mLab MongoDB test</title>

</head>
<body>
<div class="container">
    <h2>Database Results getting Routes </h2>
    <ul>
        <% results.forEach(function(r) { %>
        <li>ID:<%= r._id %></li>
        <ul>
            <li>Name:<%=r.name%></li>
            <li>Price: <%=r.price %> </li>
            <li>CUSTOMER_ID: <%=r.CUSTOMER_ID%></li>
            <li>BILLING_ID: <%=r.BILLING_ID%></li>
            <li>SHIPPING_ID:<%=r.SHIPPING_ID%></li>
            <li>DATE:<%=r.DATE%></li>
            <li>ORDER_TOTAL:<%=r.ORDER_TOTAL%></li>
        </ul>
        <br/><br/>
        <% }); %>
    </ul>
    first: <%= first %><br>
    last: <%= last %><br>
    address: <%= address %><br>
</div>

</body>
</html>
